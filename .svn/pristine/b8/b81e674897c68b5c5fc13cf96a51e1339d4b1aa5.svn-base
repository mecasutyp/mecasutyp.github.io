    var total1=0;
    var total2=0;
    //SUMAS
    $("form[name='EficienciaIn14Form'] input[name='pcs1']").val(comp1+asig1+alum1+pers1+mandos1);
    $("form[name='EficienciaIn14Form'] input[name='pcs2']").val(comp2+asig2+alum2+pers2+mandos2);

