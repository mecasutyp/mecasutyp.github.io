                if($("form[name='PertinenciaIn18Form'] input[name='radio"+cuadro+j+"']")[i].value==1){
                    total1=parseInt(total1)+parseInt($("form[name='PertinenciaIn18Form'] input[name='radio"+cuadro+j+"']")[i].value);
