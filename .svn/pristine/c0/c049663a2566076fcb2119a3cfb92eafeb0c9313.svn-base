                    $("form[name='PertinenciaIn18Form'] input[name='nivel2"+cuadro+j+"']").removeAttr("disabled");                    
                    $("form[name='PertinenciaIn18Form'] input[name='nivel3"+cuadro+j+"']").removeAttr("disabled");                    
